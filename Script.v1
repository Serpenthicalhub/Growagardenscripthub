local function getHRP(c)
    return c and c:FindFirstChild("HumanoidRootPart")
end

local function equip(t)
    if t:IsA("Tool") then
        t.Parent = localPlayer.Character
    end
end

local function flyTo(p)
    local myHRP = getHRP(localPlayer.Character)
    local theirHRP = getHRP(p.Character)
    if myHRP and theirHRP then
        myHRP.CFrame = theirHRP.CFrame + offset
    end
end

